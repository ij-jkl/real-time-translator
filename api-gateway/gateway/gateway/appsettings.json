{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "pythonService": {
        "ClusterId": "pythonCluster",
        "Match": {
          "Path": "/python/{**catch-all}"
        }
      },
      "wsAudio": {
        "ClusterId": "audioStreamingCluster",
        "Match": {
          "Path": "/ws/audio"
        }
      }
    },
    "Clusters": {
      "pythonCluster": {
        "Destinations": {
          "python1": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "audioStreamingCluster": {
        "Destinations": {
          "audio1": {
            "Address": "http://localhost:9000/"
          }
        }
      }
    }
  }
}
